{
  "samba-container-config": "v0",
  "configs": {
    "demo": {
      "shares": [
        "share"
      ],
      "globals": [
        "default"
      ],
      "instance_name": "SAMBA"
    },
    "demo2": {
      "shares": [
        "one",
        "two"
      ],
      "globals": [
        "default"
      ],
      "instance_name": "MYSERVER"
    },
    "demo3": {
      "shares": [
        "one",
        "two"
      ],
      "globals": [
        "default"
      ],
      "instance_name": "OTHERSERVER"
    },
    "wbtest": {
      "shares": [
        "share"
      ],
      "globals": [
        "noprinting",
        "wbtest"
      ],
      "instance_name": "WB1"
    }
  },
  "shares": {
    "share": {
      "options": {
        "path": "/share",
        "read only": "no"
      }
    },
    "one": {
      "options": {
        "path": "/mnt/one",
        "read only": "no"
      }
    },
    "two": {
      "options": {
        "path": "/mnt/two",
        "read only": "no"
      }
    }
  },
  "globals": {
    "default": {
      "options": {
        "security": "user",
        "server min protocol": "SMB2",
        "load printers": "no",
        "printing": "bsd",
        "printcap name": "/dev/null",
        "disable spoolss": "yes",
        "guest ok": "no"
      }
    },
    "noprinting": {
      "options": {
        "load printers": "no",
        "printing": "bsd",
        "printcap name": "/dev/null",
        "disable spoolss": "yes"
      }
    },
    "wbtest": {
      "options": {
        "log level": "10",
        "security": "ads",
        "workgroup": "ZZZ-BEST",
        "realm": "ZZZ-BEST.X.ASYNCHRONO.US",
        "server min protocol": "SMB2",
        "idmap config * : backend": "autorid",
        "idmap config * : range": "2000-9999999",
        "winbind use default domain": "yes"
      }
    }
  },
  "_users": {
    "all_entries": [
      {
        "name": "sambauser",
        "password": "samba"
      },
      {
        "name": "otheruser",
        "password": "letMe1n"
      }
    ]
  },
  "_hello_world": "100"
}
